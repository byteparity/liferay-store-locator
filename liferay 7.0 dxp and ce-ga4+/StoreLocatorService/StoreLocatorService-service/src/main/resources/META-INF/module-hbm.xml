<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.byteparity.portal.portlet.storelocator.model.StoreInformation" />
	<class name="com.byteparity.portal.portlet.storelocator.model.impl.StoreInformationImpl" table="ByteParity_StoreInformation">
		<id name="storeId" type="long">
			<generator class="assigned" />
		</id>
		<property column="uuid_" name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="storeName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="address1" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="address2" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="city" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property column="state_" name="state" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="country" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="zip" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="phone" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="latitude" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="longitude" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="createdBy" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
	</class>
</hibernate-mapping>